    Janela02(){
              
        this.setSize(1300, 1800);
        this.setTitle("Formulario Cadastro 2");
        this.setVisible(true);
        this.setIblNIPPONFLEX(new JLabel("NIPPONFLEX"));
        this.getIblNIPPONFLEX().setBounds(50, 0, 100, 30);
        this.getContentPane().setLayout(null);
        this.getContentPane().add(this.getIblNIPPONFLEX());
        
        ctnJanela02=getContentPane();
        ctnJanela02.setLayout(null);
        
        
        this.setIblDados_Pessoais(new JLabel("DADOS PESSOAIS"));
        this.getIblDados_Pessoais().setBounds(400, 0, 110, 30);
        this.getContentPane().setLayout(null);
        this.getContentPane().add(this.getIblDados_Pessoais());
       
       
        this.setIblEUMFLEX(new JLabel("EUMFLEX"));
        this.getIblEUMFLEX().setBounds(800, 0, 105, 30);
        this.getContentPane().setLayout(null);
        this.getContentPane().add(this.getIblEUMFLEX());
   
       // Nome
   
      
         this.setIblNome(new JLabel("Nome:"));
         this.getIblNome().setBounds(10, 40, 100, 25);
         JTextField Nome = new JTextField(40);
         this.setTxtNome(new JTextField(""));
         this.getTxtNome().setBounds(60, 40, 200, 25);
          
        ctnJanela02.add(iblNome);
        ctnJanela02.add(txtNome);
        setVisible(true);
        setDefaultCloseOperation(JFrame.EXIT_ON_CLOSE);
    

   
       // Data de Nascimento
   
          
           this.setIblData_de_Nascimento(new JLabel("Data de Nascimento:"));
           this.getIblData_de_Nacimento().setBounds(300, 40, 400, 25);       
           JTextField Data_de_Nascimento= new JTextField(40);
           this.setTxtData_de_Nascimento(new JTextField(""));
           this.getTxtData_de_Nascimento().setBounds(422, 40, 220, 25);
           
           ctnJanela02.add(iblData_de_Nascimento);
           ctnJanela02.add(txtData_de_Nascimento);
         
   
       // CPF
   
           
        this.setIblCpf(new JLabel("CPF:"));
        this.getIblCpf().setBounds(659, 40, 250, 25);
       
           JTextField Cpf = new JTextField(40);
           this.setTxtCpf(new JTextField(""));
           this.getTxtCpf().setBounds(704, 40, 220, 25);
           
       ctnJanela02.add(iblCpf);
       ctnJanela02.add(txtCpf);
        
        
     
        //Nacionalidade
     

        this.setIblNacionalidade(new JLabel("Nacionalidade:"));
            this.getIblNacionalidade().setBounds(10, 40, 100, 85);
           JTextField Nacionalidade = new JTextField(40);
           this.setTxtNacionalidade(new JTextField(""));
           this.getTxtNacionalidade().setBounds(100, 70,200,25);
           
